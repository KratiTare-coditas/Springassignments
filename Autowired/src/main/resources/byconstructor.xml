<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-2.5.xsd">
    <!--        <bean class="org.example.Bike" id = "vehicle">-->
    <!--        </bean>-->
    <bean class="org.example.Address" name="address" >
        <property name="hname" value="ramkripa"></property>
        <property name="city" value="mumbai"></property>
        <property name="state" value="maharashtra"></property>
    </bean>

    <bean class="org.example.Student" name="student1" autowire="constructor">
        <property name="id" value="103"></property>
        <property name="name" value="yashika"></property>
    </bean>





    <!-- <context:component-scan base-package="org.example"></context:component-scan>-->
</beans>